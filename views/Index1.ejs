    <div class="main_container">
      <div class="container12">
        <div class="side_bar ">
          <button id="side_but1" class="side_button">Editor</button>
          <button id="side_but2" class="side_button">Cache</button>
          <button id="side_but3" class="side_button">Memory</button>
          <button id="side_but4" class="side_button">Console</button>
        </div>
        <div class="holder" id="editor_id" style="display: block">

          <div class="text_holder">

            <div class="text">

              <div class="editor1">
                <div class="num" id="1"></div>
                <div class="text1">
                  <!-- <textarea name="a" id="e1" class="abc" cols="30" rows="10"></textarea> -->
                  <div id="wrapper">
                    <!-- <textarea id="mytextarea" cols="80" rows="10" placeholder="USE ME"></textarea> -->
                    <div id="mytextarea">#Write Your Code Here</div>
                  </div>
                </div>
              </div>
              <div class="editor2">
                <div class="num" id="2"></div>
                <div class="text2">
                  <!-- <textarea name="a" id="e1" class="abc" cols="30" rows="10"></textarea> -->
                  <div id="wrapper2">
                    <!-- <textarea id="mytextarea2" cols="80" rows="10" placeholder="USE ME"></textarea> -->
                    <div id="mytextarea2">#Write Your Code Here</div>
                  </div>
                </div>
              </div>
            </div>

            <div class="register">
              <!-- <button class="reg_button">Register1</button>
                        <button class="reg_button">Register2</button> -->

              <div class="btn-group" data-toggle="buttons">
                <label class="btn btn-primary">
                  <input id="radio_reg1" type="radio" name="options" id="option1" checked> Register1
                </label>
                <label class="btn btn-primary">
                  <input id="radio_reg2" type="radio" name="options" id="option2"> Register2
                </label>
              </div>

              <div class="reg_header">
                <!-- <li id="regs"> -->
                <div>
                  <ul>
                    <li id="decimal" style="display: block">
                      <table class="register1-table" cellspacing="0" id="reg1_table" style="display : block">
                        <tr>
                          <td class="t1"><button class="b1" id="reg_heading">Name</button></td>
                          <td class="t1"><input type="text" value="Value" id="val_heading"></td>
                        </tr>
                        <% for(let i=0;i<=31;i++) { %>
                          <tr>
                            <td><button class="b1" id="reg1<%= i %>">x<%= i %></button></td>
                            <td><input type="text" id="reg1Text<%= i %>" value="0" readonly></td>
                          </tr>
                          <% } %>
                      </table>

                      <table class="register2-table" cellspacing="0" id="reg2_table" style="display : none">
                        <tr>
                          <td class="t1"><button class="b1" id="reg_heading">Name</button></td>
                          <td class="t1"><input type="text" value="Value" id="val_heading"></td>
                        </tr>
                        <% for(let i=0;i<=31;i++) { %>
                          <tr>
                            <td><button class="b1" id="reg2<%= i %>">y<%= i %></button></td>
                            <td><input type="text" id="reg2Text<%= i %>" value="0" readonly></td>
                          </tr>
                          <% } %>
                      </table>

                    </li>

                  </ul>
                </div>
                <!-- </li> -->
              </div>







            </div>
          </div>
