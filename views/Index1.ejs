    <!-- for run buttons and commands -->
    <section class="Top">
      <div class="Top_buttons">
        <!-- Step forward -->
        <button class="reset" id="restart">&#8635</button>
        <button class="Step_fd">&#128898;</button>
        <!-- Step backward -->
        <button class="Step_bk">&#128896;</button>
        <button class="Run">Run</button>
          <label for="" id="forwarding">Forwarding:</label>
          <label class="forwarding_toggle">
          
          <input type="checkbox" class = "forward_input">
          <span class="slider round"></span>
        </label>
    </div>
