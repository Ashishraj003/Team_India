    <div class = "settings" style="margin-top: 1rem;">
      <div class = "setOut"></div>
      <div class = "set_align">
      <div class = "setting_header" style="display: flex;
      justify-content: space-between;
      align-items: center;">
        <a href="javascript:" class="Settings_btn btn_type2">SETTINGS</a>
        <a href="javascript:" class = "close_set btn_type1" style="margin-right: 15px;">CLOSE</a>
      </div>
      <div class = "setting_holder">
       
      <div class = "s_sidebar">
          <button id="s_side_but1" class="side_button">Latency</button>
          <button id="s_side_but2" class="side_button">Cache</button>
          <!-- <button id="s_side_but3" class="side_button">Memory</button> -->
      </div>
      <div class = "s_marker">

      </div>
      <div>
      <div class = "s_latency">
        <div >
          <label>Choose a Instruction:</label>
          <select id="latency_input" name="latencies">
            <option value="add">add</option>
            <option value="sub">sub</option>
            <option value="addi">addi</option>
            <option value="lw">lw</option>
            <option value="sw">sw</option>
            <option value="la">la</option>
            <option value="li">li</option>
            <option value="beq">beq</option>
            <option value="bne">bne</option>
            <option value="bgt">bgt</option>
            <option value="blt">blt</option>
            <option value="bgeu">bgeu</option>
            <option value="bltu">bltu</option>
            <option value="jalr">jalr</option>
            <option value="jal">jal</option>
            <option value="j">j</option>
            <option value="jr">jr</option>
        </select>
        </div>
        <div class = "s_latency_holder">
          <label>Latency  :</label>
          <input type="number" id="latencyVal" name="quantity" min="1" max="100">
      </div>
      </div>
      <div class="s_cache">
        <div class = "s_cache_holder">
          <label>Cache size  :</label>
          <select id="cache_size_input" name="cache_size">
            <option value="64">64 bytes</option>
            <option value="128">128 bytes</option>
            <option value="256">256 bytes</option>
            <option value="512">512 bytes</option>
            <option value="1024">1024 bytes</option>
            <option value="2048">2048 bytes</option>
        </select>
        <!-- </div> -->
        <!-- <div> -->
          <label>Block size  :</label>
          <select id="block_size_input" name="block_size">
            <option value="4">4 bytes</option>
            <option value="8">8 bytes</option>
            <option value="16">16 bytes</option>
            <option value="32">32 bytes</option>
            <option value="64">64 bytes</option>
          </select>
        <!-- </div> -->
        <!-- <div> -->
          <label>Associativity :</label>
          <select id="associativity" name="Associativity">
            <option value="100">Direct Associative</option>
            <option value="32">32 Way Associativity</option>
            <option value="16">16 Way Associativity</option>
            <option value="8">8 Way Associativity</option>
            <option value="4">4 Way Associativity</option>
            <option value="2">2 Way Associativity</option>
            <option value="1">Fully Associativity</option>
          </select>
        <!-- </div> -->
        <!-- <div> -->
          <label>Policy :</label>
          <select id="policy" name="Policy">
            <option value="0">LRU</option>
            <option value="1">Random</option>
          </select>
        <!-- </div> -->
        <!-- <div> -->
          <label>Memory Latency :</label>
          <input type="number" id="mem_latencyVal" name="quantity" min="1" max="200">
        <!-- </div> -->
        <!-- <div> -->
          <label>Cache Latency    :</label>
          <input type="number" id="cache_latencyVal" name="quantity" min="1" max="100">
        </div>
      </div>
      </div>
    </div>
      </div>
