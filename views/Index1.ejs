              <td><input class="mem_element_head" id="mem-0_5" type = "text" readonly = true></input></td>
            </tr>
            <% for(let i=1;i<=11;i++) { %>
              <tr>
                <% for(let j=0;j<=5;j++) { %>
                    <td><input class="mem_element col_name" id="mem-<%=i%>_<%=j%>" type = "text" readonly = true></input></td>
                  <% } %>
