    <!-- for run buttons and commands -->
    <section class="Top">
      <div class="Top_buttons_holder">
        <div class="Top_buttons">
          <!-- Step forward -->
          <button class="reset pushable" id="restart"><span class = "front">-&#8635-</span></button>
          
          <!-- Step backward -->
          <button class="Step_bk pushable"> <span class = "front">Step-Back </span></button>
          <button class="Step_fd pushable"><span class = "front">Step-Front</span></button>
          <button class="Run pushable" style = "margin-left: 20px;"><span class = "front" >Fast-Run</span></button>
          <div id="run_wait">
          <input type="number" id="run_wait_ms" name="quantity" min="10" max="1000" step="10" value = "100" style = "margin-left: 30px;">
          </div>
          <button class="Run_step pushable"><span class = "front">Run-Steps</span></button>

            
        </div>
        <div class="Top_buttons">
          <label for="" id="forwarding">Forwarding:</label>
            <label class="forwarding_toggle">
            
            <input type="checkbox" class = "forward_input">
            <span class="slider round"></span>
          </label>
        </div>
    </div>
    </section>




